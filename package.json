{
  "name": "lcov-result-merger",
  "description": "Merges multiple lcov results into one",
  "main": "index.js",
  "files": [
    "index.js",
    "bin"
  ],
  "scripts": {
    "lint": "standard",
    "test": "mocha -R spec test --bail",
    "test-coverage": "nyc --reporter=lcov npm run test",
